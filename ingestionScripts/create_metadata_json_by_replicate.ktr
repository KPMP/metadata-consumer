	FASTQ_fastQFileNamePairedEnd:row[getInputRowMeta().indexOfValue("Associated FASTQ File Name (Paired End):")]?String(row[getInputRowMeta().indexOfValue("Associated FASTQ File Name (Paired End):")]):null,
	BAM_bamFileName:row[getInputRowMeta().indexOfValue("Associated BAM File Name:")]?String(row[getInputRowMeta().indexOfValue("Associated BAM File Name:")]):null,
	Expression_Matrix_barcodesFileName:row[getInputRowMeta().indexOfValue("Barcodes File Name:")]?String(row[getInputRowMeta().indexOfValue("Barcodes File Name:")]):null,
	Expression_Matrix_matrixFileName:row[getInputRowMeta().indexOfValue("Matrix File Name:")]?String(row[getInputRowMeta().indexOfValue("Matrix File Name:")]):null,
	Expression_Matrix_genesFileName:row[getInputRowMeta().indexOfValue("Genes/Features File Name:")]?String(row[getInputRowMeta().indexOfValue("Genes/Features File Name:")]):null
};
var fieldMapString = getVariable("fieldMap", "");
var fieldMap = fieldMapString.split("|");

for(var i = 0; i &lt; fieldMap.length; i++) {
	var fieldInfo = fieldMap[i];
	var dataType = 'String';
	var fieldArr = fieldInfo.split(",");
    var fileType = fieldArr[0];
	var fieldName = fieldArr[1];
	var fieldJsonProperty = fieldArr[2];
