	replicateID:!isNaN(row[getInputRowMeta().indexOfValue("Replicate ID")])?(Number(row[getInputRowMeta().indexOfValue("Replicate ID")])).toString():String(row[getInputRowMeta().indexOfValue("Replicate ID")]),
};
var fastQFiles = {};
var bamFiles = {};
var expressionMatrixFiles = {};
var fieldMapString = getVariable("fieldMap", "");
var fieldMap = fieldMapString.split("|");

for(var i = 0; i &lt; fieldMap.length; i++) {
	var fieldInfo = fieldMap[i];
	var dataType = 'String';
	var fieldArr = fieldInfo.split(",");
    var fileType = fieldArr[0];
	var fieldName = fieldArr[1];
	var fieldJsonProperty = fieldArr[2];
	var fieldIndex = getInputRowMeta().indexOfValue(fieldName);
    var metadata = getInputRowMeta().getValueMeta(fieldIndex);
	if (metadata != null) {
		dataType = metadata.getTypeDesc();
	}
	var fieldVal = row[getInputRowMeta().indexOfValue(fieldName)];
	
	if (fieldVal != null) {
		if (dataType == 'Number' || dataType == 'Integer') {
			fieldVal = Number(fieldVal);
    	} else {
			fieldVal = !isNaN(fieldVal)?(Number(fieldVal)).toString():String(fieldVal);
		}
	}
	
if (fileType == 'FASTQ') {
		fastQFiles['fastQFileName'] = row[getInputRowMeta().indexOfValue("Associated FASTQ File Name:")]?String(row[getInputRowMeta().indexOfValue("Associated FASTQ File Name:")]):null;
		fastQFiles['fastQFileNamePairedEnd'] = row[getInputRowMeta().indexOfValue("Associated FASTQ File Name (Paired End):")]?String(row[getInputRowMeta().indexOfValue("Associated FASTQ File Name (Paired End):")]):null;
    	fastQFiles[fieldJsonProperty] = fieldVal;
	}
	else if (fileType == 'BAM') {
		bamFiles['bamFileName'] = row[getInputRowMeta().indexOfValue("Associated BAM File Name:")]?String(row[getInputRowMeta().indexOfValue("Associated BAM File Name:")]):null;
    	bamFiles[fieldJsonProperty] = fieldVal;
	}
	else if (fileType == 'Expression Matrix') {
		expressionMatrixFiles['barcodesFileName'] = row[getInputRowMeta().indexOfValue("Barcodes File Name:")]?String(row[getInputRowMeta().indexOfValue("Barcodes File Name:")]):null;
		expressionMatrixFiles['matrixFileName'] = row[getInputRowMeta().indexOfValue("Matrix File Name:")]?String(row[getInputRowMeta().indexOfValue("Matrix File Name:")]):null;
		expressionMatrixFiles['genesFileName'] = row[getInputRowMeta().indexOfValue("Genes/Features File Name:")]?String(row[getInputRowMeta().indexOfValue("Genes/Features File Name:")]):null;
    	expressionMatrixFiles[fieldJsonProperty] = fieldVal;
	}
	else {
		sampleObj[fieldJsonProperty] = fieldVal;
	}
};
sampleObj['fastQFiles'] = fastQFiles;
sampleObj['bamFiles'] = bamFiles;
sampleObj['expressionMatrixFiles'] = expressionMatrixFiles;
